    </category>
    <category name="Table content">
      <block type="table">
        <statement name="content">
          <block type="tr">
            <statement name="content">
              <block type="td">
                <next>
                  <block type="td"></block>
                </next>
              </block>
            </statement>
            <next>
              <block type="tr">
                <statement name="content">
                  <block type="td">
                    <next>
                      <block type="td"></block>
                    </next>
                  </block>
                </statement>
              </block>
            </next>
          </block>
        </statement>
